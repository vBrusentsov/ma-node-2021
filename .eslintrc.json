{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "plugins": ["prettier"],
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "quotes": ["error", "single"],
        "prettier/prettier": "error"
    }
}
